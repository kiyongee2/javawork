package classes.constructor;

public class PersonTest {

	public static void main(String[] args) {
		// Person 객체 생성과 사용
		// 기본 생성자(Constructor)로 생성
		Person p1 = new Person();
		p1.name = "우영우";
		p1.height = 165.3f;
		p1.weight = 51.2f;
		
		p1.showPersonInfo();
		
		System.out.println("=============================");

	}

}
